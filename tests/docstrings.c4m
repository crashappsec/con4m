"""
This is my module.
Here is one of its two doc strings.
"""
"Here's it's other doc."

extern callecho(a: ptr) -> cvoid {
  "This has some docs too."
  
  local: print(string) -> void
  pure: false
}

extern echoanint(a: cint) -> cvoid {
  local: print(int) -> void
  pure: false
}

func fib(x) {
  "Also doc'd."
  "And double doc'd."
  
  switch x {
    case 0:
        return 0
    case 1:
        x = x - x + 1
        return x
    else:
        return fib(x - 2) + fib(x - 1)
  }
}

x = fib(10)
assert x == 55
